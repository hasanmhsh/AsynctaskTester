<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="horizontal" xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.hms.sra.asynctasktester.MainActivity">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="enqueue"
        android:onClick="onEnqueueClickListener"/>
    <EditText
        android:id="@+id/preetid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="3"/>
    <EditText
        android:id="@+id/etid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="3"/>

    <EditText
        android:id="@+id/petid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="3"/>

</LinearLayout>
